<h1><%= title %></h1>
<form method="post">
  <label for="password">Enter secret password: </label>
  <input type="password" id="password" name="password" required />
  <button type="submit">Submit</button>
</form>
<% if(locals.errors) { %>
<ul>
  <% for(let error of errors) {%>
  <li><%= error.msg %></li>
  <% } %>
</ul>
<% } %>
